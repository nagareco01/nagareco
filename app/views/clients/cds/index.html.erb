<div>
	<h2>ジャンル</h2>
	<% @cds.each do |cd| %>
		<div><%= link_to clients_cd_path(cd.id) do %>
	    <%= attachment_image_tag cd, :image, :fill, 40, 40, format: 'jpeg' ,fallback: "noimage.jpg" ,:size =>'40x40' %>
	    <% end %></div>
		<div><%= cd.artist.artist_name %></div>
	    <div><%= cd.name %></div>
	    <div><%= cd.price %></div>
    <% end %>
</div>
