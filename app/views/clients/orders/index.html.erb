<h1>注文履歴一覧</h1>

<% @orders.each do |order| %>
<div>
<%= order.created_at.strftime("%Y-%m-%d") %>
<%= order.payment %>
<%= order.shipment_status %>
</div>
<div>
<%= order.order_items.each do |item| %>
<%= item.cd.name %>
<%= item.quantity %>
<%= item.purchased_price %>
<% end %>
</div>
<% end %>