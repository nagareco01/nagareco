<div class="container">
	<h2>ジャンル</h2>
	<% @cds.each do |cd| %>
		<%= link_to admins_cd_path(cd.id) do %>
	    <%= attachment_image_tag cd, :image, :fill, 40, 40, format: 'jpeg' ,fallback: "noimage.jpg" ,:size =>'40x40' %>
	    <% end %>
		<%= cd.artist.artist_name %>
	    <%= cd.name %>
	    <%= cd.price %>
    <% end %>
</div>