<h2>アーティスト、レーベル、ジャンル追加</h2>


<%= form_for @artist, url: admins_cd_add_info_path do |f| %>
	<div>
		<h4>アーティスト名</h4>
		<%= f.text_field :artist_name, required:true %>
	</div>
	<div>
	<%= f.submit "追加"%>
	</div>
<% end %>

<%= form_for @label, url: admins_cd_add_info_path do |f| %>
	<div>
		<h4>レーベル名</h4>
	<%= f.text_field :label_name, required:true %>
	</div>
	<div>
	<%= f.submit "追加"%>
	</div>
<% end %>

<%= form_for @genre, url: admins_cd_add_info_path do |f| %>
	<div>
		<h4>ジャンル名</h4>
	<%= f.text_field :genre_name, required:true %>
	</div>
	<div>
	<%= f.submit "追加"%>
	</div>
<% end %>