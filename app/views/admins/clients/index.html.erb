<div class="title">
	<h2 class="space">顧客一覧</h2>
	<div class="client-pepple">
		<% @clients.each do |client| %>
			<h4 class="client-man"><%= client.id %> . <%= link_to (client.last_name + client.first_name), admins_client_path(client.id) %></h4>
		<% end %>
	</div>
	<div class="kaminari-page">
		<%= paginate @clients %>
	</div>
	<span id="back">
		<a href="#">
			<%= image_tag "arrow.png"  %>
		</a>
	</span>
</div>